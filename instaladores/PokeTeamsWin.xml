    <readyToInstallActionList>
        <autodetectJava>
            <promptUser>1</promptUser>
            <onErrorActionList>
                <launchBrowser>
                    <url>https://www.oracle.com/es/java/technologies/downloads/</url>
                </launchBrowser>
            </onErrorActionList>
            <validVersionList>
                <validVersion>
                    <bitness></bitness>
                    <maxVersion>23</maxVersion>
                    <minVersion>23</minVersion>
                    <requireJDK>1</requireJDK>
                    <vendor></vendor>
                </validVersion>
            </validVersionList>
        </autodetectJava>
    </readyToInstallActionList>
    <postInstallationActionList>
        <setWindowsACL>
            <action>allow</action>
            <files>${installdir}</files>
            <recurseObjects>1</recurseObjects>
        </setWindowsACL>
        <createJavaLaunchers>
            <destination>${installdir}</destination>
            <javaLauncherList>
                <javaLauncher>
                    <allowCommandLineArguments>1</allowCommandLineArguments>
                    <arguments></arguments>
                    <binaryName>PokeTeams.exe</binaryName>
                    <classpath></classpath>
                    <jarFile>PokeTeams.jar</jarFile>
                    <mainClass></mainClass>
                    <preferredJavaBinary>${java_executable}</preferredJavaBinary>
                    <runInBackground>0</runInBackground>
                    <runInConsole>1</runInConsole>
                    <useLauncherDirectoryAsWorkingDirectory>1</useLauncherDirectoryAsWorkingDirectory>
                    <usePropertiesFile>0</usePropertiesFile>
                    <vmParameters>--module-path lib --add-modules javafx.controls,javafx.fxml,javafx.graphics,java--module-path lib --add-modules javafx.controls,javafx.fxml,javafx.graphics,javafx.swing,javafx.web,org.controlsfx.controls,org.apache.commons.logging -cp "lib\gson-2.11.0.jar;lib*;PokeTeams" com.fcm.pokeTeams.Main.11.0.jar com.fcm.pokeTeams.Main</vmParameters>
                    <windowsExecutableIcon>C:\instalador\distributionsWin\logo-ico.ico</windowsExecutableIcon>
                    <windowsResourceFileVersion>1.0.0.0</windowsResourceFileVersion>
                    <workingDirectory>${installdir}</workingDirectory>
                </javaLauncher>
            </javaLauncherList>
        </createJavaLaunchers>
    </postInstallationActionList>
    <enableRollback>0</enableRollback>
    <enableTimestamp>1</enableTimestamp>
    <vendor>Francisco Crespo Mart√≠n</vendor>
